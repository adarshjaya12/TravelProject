<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <environment include="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="~/css/main.css" />
        <link href="https://fonts.googleapis.com/css?family=Libre+Franklin:300,400,500,600,800,900" rel="stylesheet">
    </environment>
</head>
<body>
    @RenderBody()

    <environment include="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <script src="~/dist/home-form-bundle.js"></script>
        <script src="~/dist/home-map-bundle.js"></script>
        <script src="~/dist/global-bundle.js"></script>

    </environment>
    @RenderSection("Scripts", required: false)
</body>
</html>
